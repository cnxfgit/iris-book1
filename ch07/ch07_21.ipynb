   "source": [
    "def transpose_2(matrix):\n",
    "    transposed = []\n",
    "    rows = len(matrix)\n",
    "    cols = len(matrix[0])\n",
    "\n",
    "    transposed = [[matrix[j][i] for j in range(rows)]\n",
    "                  for i in range(cols)]\n",
    "    return transposed"
   ]
  },
